<% @blogs.each do |blog| %>
<div class="breaker">
  <div class="breaker-internal">
  </div>
</div>
<div class="md-intro">
  <!-- @latest_blog -->
  <div class="md-intro-l">
    <h1>Mama Duet Meets...</h1>
    <div class="intro-image">
      <%= image_tag blog.images.first %>
    </div>
    <div class="link left">
    <%= link_to "Read this post", blog_path(blog) %>
    </div>
  </div>
  <div class="md-intro-r">
    <%= link_to blog_path(blog) do %>
    <h1>...<%=  blog.title %></h1>
    <% end %>
    <div class="text long">
    <%= blog.mama_intro %>
    </div>
  </div>
</div>

<% end %>
